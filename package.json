{
  "name": "@yourname/clawdbot-wecom",
  "version": "1.0.0",
  "type": "module",
  "description": "OpenClaw WeChat Work (企业微信) channel plugin",
  "license": "MIT",
  "files": [
    "index.ts",
    "src",
    "openclaw.plugin.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourname/clawdbot-wecom.git"
  },
  "keywords": [
    "openclaw",
    "wecom",
    "wechat",
    "企业微信",
    "work",
    "wxwork",
    "chatbot",
    "ai",
    "claude"
  ],
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "wecom",
      "label": "WeChat Work",
      "selectionLabel": "WeChat Work (企业微信)",
      "docsPath": "/channels/wecom",
      "docsLabel": "wecom",
      "blurb": "企业微信 enterprise messaging.",
      "aliases": [
        "wxwork"
      ],
      "order": 80
    },
    "install": {
      "npmSpec": "@yourname/clawdbot-wecom",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.0",
    "body-parser": "^1.20.0",
    "xml2js": "^0.6.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "openclaw": "2026.1.29",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.29"
  }
}
